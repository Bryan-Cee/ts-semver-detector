{
  "name": "typeshift",
  "version": "0.1.0",
  "description": "A CLI tool to analyze changes between TypeScript definition files and determine semantic version increments",
  "main": "dist/index.js",
  "bin": {
    "typeshift": "./dist/cli/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bryan-Cee/typeshift.git"
  },
  "homepage": "https://github.com/Bryan-Cee/typeshift#readme",
  "bugs": {
    "url": "https://github.com/Bryan-Cee/typeshift/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "start": "ts-node src/cli/index.ts",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run lint && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "keywords": [
    "typescript",
    "semver",
    "diff",
    "analyzer",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "cosmiconfig": "^9.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0"
  }
}
